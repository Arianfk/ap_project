            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="teacherNameLabel" layoutX="14.0" layoutY="57.0" text="Teacher Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button onAction="#onInfoButtonClicked" fx:id="infoButton" layoutX="403.0" layoutY="23.0" mnemonicParsing="false" prefWidth="82.0" text="info" />
      <Button onAction="#onEditButtonClicked" fx:id="editButton" layoutX="404.0" layoutY="54.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="82.0" text="edit" />
   </children>
</AnchorPane>
