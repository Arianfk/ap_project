<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.ap.NewTeacherPane"
            prefHeight="400.0" prefWidth="600.0">

    <TextField fx:id="usernameField" layoutX="15.0" layoutY="14.0" promptText="Username" />
    <TextField fx:id="passwordField" layoutX="15.0" layoutY="46.0" promptText="Password" />
    <TextField fx:id="nameField" layoutX="15.0" layoutY="78.0" promptText="Name" />
    <DatePicker fx:id="birthDatePicker" layoutX="15.0" layoutY="110.0" promptText="Birthday" />
    <TextField fx:id="codeField" layoutX="15.0" layoutY="143.0" promptText="National Code" />
    <ComboBox fx:id="degreeCombo" layoutX="15.0" layoutY="175.0" prefWidth="150.0" promptText="Degree" />
    <Button onAction="#save" layoutX="15.0" layoutY="240.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Save" />


</AnchorPane>
