<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="listView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="removeButton" onAction="#removeButtonClicked" layoutX="14.0" layoutY="357.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="200.0" text="Remove Session" />
      <ComboBox fx:id="dayCombo" layoutX="14.0" layoutY="223.0" prefHeight="26.0" prefWidth="200.0" promptText="Day" />
      <TextField fx:id="hourField" layoutX="14.0" layoutY="256.0" prefHeight="26.0" prefWidth="95.0" promptText="Hour" />
      <TextField fx:id="minuteField" layoutX="118.0" layoutY="256.0" prefHeight="26.0" prefWidth="95.0" promptText="Minute" />
      <TextField fx:id="lengthField" layoutX="14.0" layoutY="290.0" prefHeight="26.0" prefWidth="200.0" promptText="Length In Minutes" />
      <Button fx:id="addButton" onAction="#addButtonClicked" layoutX="14.0" layoutY="323.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="200.0" text="Add Session" />
   </children>
</AnchorPane>
