<AnchorPane prefHeight="459.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ap.LogInViewController">
    <TextField fx:id="usernameField" layoutX="220.0" layoutY="131.0" prefHeight="26.0" prefWidth="160.0" promptText="Username" />
    <TextField fx:id="passwordField" layoutX="220.0" layoutY="164.0" prefHeight="26.0" prefWidth="160.0" promptText="Password" />
    <Label fx:id="errorLabel" alignment="CENTER" layoutX="161.0" layoutY="196.0" prefHeight="18.0" prefWidth="277.0" textAlignment="CENTER" textFill="RED" />
    <ImageView fx:id="captchaImageView" fitHeight="90.0" fitWidth="160.0" layoutX="220.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true" />
    <Button onAction="#refresh" layoutX="274.0" layoutY="286.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="52.0" text="change">
        <font>
            <Font size="10.0" />
        </font>
    </Button>
    <TextField fx:id="captchaField" layoutX="225.0" layoutY="320.0" promptText="captcha" />
    <Label fx:id="captchaErrorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="200.0" layoutY="352.0" prefHeight="18.0" prefWidth="199.0" textFill="RED" />
    <Button fx:id="logInButton" layoutX="248.0" layoutY="383.0" mnemonicParsing="false" onAction="#OnLogInClicked" prefHeight="26.0" prefWidth="104.0" text="Log In!" />
